/* 注意事项样式开头 */
  .row{
    width: 650rpx;
    padding: 30rpx 50rpx;
    border-bottom: 1px solid #ccc;
  }

  .message_content{
    display: flex;
    justify-content: space-between;
    width: 100%;
    overflow: hidden;
    border-radius: 20rpx;
  }

  .message_content .main_message {
    display: flex;
    flex-direction: column;
    flex-grow: 1;
    width: 100%;
    margin-left: -450rpx;
    background-color: #fff;
    padding-left: 20rpx;
  }

  .M_firstrow{
    display: flex;
    margin-top:15rpx;
  }
  .showname{
    width: 350rpx;
    height: 40rpx;
    font-size: 36rpx;
    font-weight: 800;
  }

  .showtime{
    margin-top: 10rpx;
    height: 30rpx;
    font-size:25rpx;
    color: #707070;
  }

  .showcontent{
    margin-top: 10rpx;
    font-size: 30rpx;
    display:  -webkit-box;
    word-break: break-all;
    width: 600rpx;
  }

  .showcontent.hide{
    /*规定子元素的排列方向 */
    -webkit-box-orient: vertical;
    /*限制文本显示的行数  */
    -webkit-line-clamp: 3;
    /*实现多余的文字隐藏并用省略号来代表被隐藏的文字  */
    overflow: hidden;
    text-overflow: ellipsis;
  }
  /* 侧滑样式 */
  .message_content .remove {
    flex-grow: 0;
    display: flex;
    align-items: center;
    font-size: 35rpx;
    justify-content: center;
    width: 150rpx;
    color: #fff;
    background-color: #fe3e2f;
  }
  .message_content .imp{
    flex-grow: 0;
    display: flex;
    align-items: center;
    font-size: 35rpx;
    justify-content: center;
    width: 150rpx;
    color: #fff;
    background-color: rgba(250, 152, 40, 0.87);
  }
  .message_content .topping {
    flex-grow: 0;
    display: flex;
    align-items: center;
    font-size: 35rpx;
    justify-content: center;
    width: 150rpx;
    color: #fff;
    background-color: rgba(40, 113, 250, 0.87);
  }

  .message_content .main_message, .message_content .topping, .message_content .remove, .message_content .imp{
    transform: translateX(450rpx);
    transition: all 0.5s;
  }
  .touch-move-active .main_message,.touch-move-active .remove ,.touch-move-active .topping, .touch-move-active .imp{
    -webkit-transform: translateX(0);
    transform: translateX(0);
  }
/* 注意事项样式结尾 */


