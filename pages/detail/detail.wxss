      url('data:font/woff;charset=utf-8;base64,') format('woff'),
      url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

.iconfont {
  font-family: "iconfont" !important;
